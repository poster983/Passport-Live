{
  "name": "passport-live",
  "version": "0.1.1",
  "description": "Passport-Live is a modern web app for schools that helps them manage passes.",
  "author": "Joseph Hassell <hi@josephhassell.com> (http://josephhassell.com/)",
  "keywords": [
    "passport",
    "pass",
    "school",
    "student",
    "express"
  ],
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www -e js,mustache",
    "node": "nodemon ./bin/www",
    "db": "rethinkdb serve",
    "start:dbEdit": "chateau",
    "configure:db": "node ./bin/configure",
    "test": "export NODE_ENV=tests && nyc mocha",
    "test-gitlab": "export NODE_ENV=gitlab && nyc mocha",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "generate-docs": "jsdoc --configure config/jsdoc.json --verbose",
    "postinstall": "./node_modules/bower/bin/bower install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/poster983/Passport-Live"
  },
  "bugs": {
    "url": "https://github.com/poster983/Passport-Live/issues"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.17.2",
    "bower": "^1.8.0",
    "config": "latest",
    "connect-ensure-login": "latest",
    "connect-flash": "~0.1.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.3",
    "debug": "~2.6.0",
    "express": "~4.15.3",
    "express-session": "latest",
    "forever-monitor": "latest",
    "humanparser": "^1.3.0",
    "jsonwebtoken": "^7.4.1",
    "moment": "^2.18.1",
    "morgan": "~1.8.2",
    "mustache-express": "1.2.4",
    "node-sass-middleware": "0.9.8",
    "nodemon": "latest",
    "optional": "^0.1.3",
    "passport": "0.3.2",
    "passport-jwt": "^2.2.1",
    "passport-local": "latest",
    "rethinkdb": "latest",
    "serve-favicon": "~2.4.3",
    "session-file-store": "^1.0.0",
    "shortid": "^2.2.8",
    "socket.io": "^2.0.0",
    "ssarv": "latest"
  },
  "license": "AGPL-3.0",
  "snyk": true,
  "devDependencies": {
    "chai": "^4.0.2",
    "codecov": "^2.2.0",
    "jsdoc": "^3.4.3",
    "loke-jsdoc-theme": "^2.1.0",
    "mocha": "^3.4.1",
    "nyc": "^10.3.2",
    "request": "^2.81.0"
  }
}
