{
  "name": "passport-live",
  "version": "0.12.2",
  "description": "Passport-Live is a modern web app for schools that helps them manage passes.",
  "author": "Joseph Hassell <hi@josephhassell.com> (http://josephhassell.com/)",
  "keywords": [
    "passport",
    "pass",
    "school",
    "student",
    "express"
  ],
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www -e js,mustache --trace-sync-io",
    "production": "export NODE_ENV=production && ./node_modules/forever/bin/forever ./bin/www",
    "db": "rethinkdb serve --http-port 3060",
    "start:dbEdit": "chateau",
    "debug": "echo fs.inotify.max_user_watches=582222 | sudo tee -a /etc/sysctl.conf && sudo sysctl -p",
    "configure:db": "node ./bin/configure",
    "configure:root": "node ./bin/createAdminAccount",
    "test": "export NODE_ENV=tests && nyc mocha",
    "test-gitlab": "export NODE_ENV=gitlab && nyc mocha",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "generate-docs": "jsdoc --configure config/jsdoc.json --verbose",
    "snyk-protect": "snyk protect",
    "prepare": "npm run build:production && npm run snyk-protect",
    "build:production": "webpack --progress --config webpack.prod.js",
    "build": "webpack --progress --colors --config webpack.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/poster983/Passport-Live"
  },
  "bugs": {
    "url": "https://github.com/poster983/Passport-Live/issues"
  },
  "dependencies": {
    "@polymer/app-layout": "3.0.0-pre.12",
