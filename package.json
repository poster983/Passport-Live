{
  "name": "passport-live",
  "version": "0.9.0",
  "description": "Passport-Live is a modern web app for schools that helps them manage passes.",
  "author": "Joseph Hassell <hi@josephhassell.com> (http://josephhassell.com/)",
  "keywords": [
    "passport",
    "pass",
    "school",
    "student",
    "express"
  ],
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www -e js,mustache",
    "production": "export NODE_ENV=production && ./node_modules/forever/bin/forever ./bin/www",
    "db": "rethinkdb serve --http-port 3060",
    "start:dbEdit": "chateau",
    "debug": "echo fs.inotify.max_user_watches=582222 | sudo tee -a /etc/sysctl.conf && sudo sysctl -p",
    "configure:db": "node ./bin/configure",
    "configure:root": "node ./bin/createAdminAccount",
    "test": "export NODE_ENV=tests && nyc mocha",
    "test-gitlab": "export NODE_ENV=gitlab && nyc mocha",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "generate-docs": "jsdoc --configure config/jsdoc.json --verbose"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/poster983/Passport-Live"
  },
  "bugs": {
    "url": "https://github.com/poster983/Passport-Live/issues"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.17.2",
    "compression": "^1.7.0",
    "config": "latest",
    "connect-ensure-login": "latest",
    "connect-flash": "~0.1.1",
    "cookie-parser": "~1.4.3",
    "cookie-session": "^2.0.0-beta.2",
    "cors": "^2.8.3",
    "debug": "~2.6.0",
    "excel-as-json": "^2.0.1",
    "express": "~4.15.3",
    "express-session": "latest",
    "forever": "^0.15.3",
    "geopattern": "^1.2.3",
    "greenlock-express": "^2.0.11",
    "humanparser": "^1.5.0",
    "jdenticon": "^1.6.0",
    "jquery.stellar": "^0.6.2",
    "jsonwebtoken": "^7.4.1",
    "loader-message": "latest",
    "moment": "^2.18.1",
    "morgan": "~1.8.2",
    "mustache-express": "1.2.4",
    "node-sass-middleware": "0.9.8",
    "nodemon": "latest",
    "optional": "^0.1.3",
    "passport": "0.3.2",
    "passport-google-oauth20": "^1.0.0",
    "passport-jwt": "^2.2.1",
    "passport-local": "latest",
    "requirejs": "^2.3.3",
    "rethinkdb": "latest",
    "serve-favicon": "~2.4.3",
    "session-file-store": "^1.0.0",
    "shortid": "^2.2.8",
    "socket.io": "^2.0.0",
    "ssarv": "latest",
    "ua-parser-js": "^0.7.14",
    "underscore": "^1.8.3",
    "validator": "^8.0.0"
  },
  "license": "AGPL-3.0",
  "snyk": true,
  "devDependencies": {
    "chai": "^4.0.2",
    "codecov": "^2.2.0",
    "jsdoc": "^3.4.3",
    "loke-jsdoc-theme": "^2.1.0",
    "mocha": "^3.5.0",
    "nyc": "^10.3.2",
    "request": "^2.81.0"
  }
}
