    "@polymer/paper-listbox": "3.0.0-pre.12",
    "@polymer/paper-spinner": "3.0.0-pre.12",
    "@polymer/paper-styles": "3.0.0-pre.12",
    "@polymer/paper-tooltip": "3.0.0-pre.12",
    "@polymer/polymer": "3.0.0-pre.12",
